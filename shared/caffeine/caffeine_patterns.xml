<?xml version="1.0" encoding="UTF-8"?>

<profile name="Caffeine" version="3.4-SNAPSHOT">

		<!-- 
			Pattern rule appliance by priority :
			0 - locker (1004)
			1 - stack_size_interest (1003)
			2 - exclude_thread_name (1002)
			3 - exclude (1001)
			4 - locker  (7)
			5 - function  (6)
			6 - function_discovery  (5)
			7 - executor  (4)
			8 - executor_thread_name (3)
			9 - operation (2)
			10 - operation_discovery (1)
			Exclude, function, operation and executor patterns priorities can be overriden by pattern attribute.
	 	-->
		<patterns>
			<!-- In pattern attribute, use semicolon separator to declare multiple line patterns -->
			<functions>
				<function pattern="com.github.benmanes.caffeine.cache.BoundedLocalCache.expireEntries" name="Caffeine cache expire entries"/>
				<function pattern="com.github.benmanes.caffeine.cache.LocalManualCache.get" name="Caffeine cache get"/>
			</functions>
			<operations>
				<!-- Operation patterns apply for the first 20 lines of the thread dump                	 -->
				<!-- Exception : patterns ending with '.' apply for the first 2 lines of the thread dump -->
				<!--operation pattern="com.github.benmanes.caffeine.cache.LocalManualCache.get" name="Caffeine cache get" type="Collection"/-->
			</operations>
			<lockers>
				<!-- Locker patterns apply for the first 20 lines of the thread dump                 -->
  				<!-- locker pattern="java.util.concurrent.locks.LockSupport.park" name="LockSupport"/-->
			</lockers>
			<executors>
				<!-- Executor patterns must be available within the 20 first lines of the thread dump -->
				<executor pattern="com.github.benmanes.caffeine.cache.BoundedLocalCache$PerformCleanupTask.exec" name="Caffeine cache cleaner"/>
			</executors>
			<executor_thread_names>
				<!-- Executor pattern based on the thread name. Regular expression recommended -->
				<!-- executor_thread_name pattern="Connection evictor" name="Connection watchdog"/-->
			</executor_thread_names>
			<excludes>
				<!-- If stack size is equal or below the limit (optional size attribute), thread stack is excluded -->
				<!--exclude pattern="java.util.concurrent.DelayQueue.take;org.apache.curator.framework.imps.CuratorFrameworkImpl.backgroundOperationsLoop;org.apache.curator.framework.imps.CuratorFrameworkImpl.access;org.apache.curator.framework.imps.CuratorFrameworkImpl$4.call;java.util.concurrent.FutureTask.run" name="Curator background operation loop"/-->
			</excludes>
			<exclude_thread_names>
				<!-- Exclude stack based on thread name. Regular expression based recommended -->
				<!-- If stack size is equal or below the limit (optional size attribute), thread stack is excluded -->
				<!-- <exclude_thread_name pattern="Curator-ConnectionStateManager" name="Curator connection state manager 2"/> -->
			</exclude_thread_names>
		</patterns>

</profile>
