<?xml version="1.0" encoding="UTF-8"?>

		<sheet type="sequence" name="Operations" link_type="Ope">
		
			<!-- Sheet description. Use &#10; as carriage return -->
			<description>Displays AMQ and KAHADB low level activity.</description>
		
			<headers>
				<tendency_operation/>
				<tendency_contention_type/>
			</headers>
		
			<row_headers>
				<thread_id action_link="true"/>
				<thread_name/>
			</row_headers>
		
			<display>

				<advanced_operation>
					<highlight name="(OTBI)" color="GREY_25_PERCENT"/>
					<highlight name="Register destination" color="LAVENDER"/>
					<highlight name="Wait for other thread(s) to complete their operation" color="LAVENDER"/>
					<highlight name="KAHADB Journal page file flush writing" color="LIGHT_TURQUOISE"/>
				</advanced_operation>

				<!-- Java Lock display -->
				<lock_state color="ORANGE" lock_owner="RGB-209-47-109" lock_owner_and_locked="RGB-174-76-169" deadlock="RED" suspended="RED" stats="true"/>

				<!-- Stack trace display as cell comment. Adds overhead processing -->
				<stack/>

			</display>
		</sheet>
		
